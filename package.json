{
  "name": "notie-js",
  "description": "A clean and simple notification plugin (alert/growl style) for javascript, with no dependencies.",
  "author": "Jared Reich",
  "version": "2.0.1",
  "main": "dist/notie.js",
  "scripts": {
    "start": "watchify -v --standalone notie src/main.js -o dist/notie.js",
    "build": "browserify --standalone notie src/main.js > dist/notie.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredreich/notie.js.git"
  },
  "keywords": [
    "tip"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaredreich/notie.js/issues"
  },
  "homepage": "https://jaredreich.com/projects/notie.js",
  "devDependencies": {
    "browserify": "^12.0.1",
    "watchify": "^3.6.1"
  }
}
